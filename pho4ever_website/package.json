{
  "name": "pho4ever_website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "vite",
    "build": "tsc --noEmit && npm run build:css && vite build --emptyOutDir",
    "serve": "vite preview",
    "format": "prettier ./src -w",
    "lint": "eslint ./src --fix",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./public/output.css"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tanstack/router-plugin": "^1.73.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.11.0",
    "tailwindcss": "^3.4.14",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.8",
    "vite-plugin-mkcert": "^1.17.6"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@tanstack/react-router": "^1.73.1",
    "@tanstack/router-devtools": "^1.73.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-material-ui-carousel": "^3.4.2"
  }
}
